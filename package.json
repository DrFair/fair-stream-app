{
  "name": "fair-stream-app",
  "description": "An app to help with stream overlays and more",
  "version": "0.1.0",
  "author": "Mads Skovgaard",
  "private": true,
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.16",
    "@fortawesome/free-solid-svg-icons": "^5.8.0",
    "@fortawesome/react-fontawesome": "^0.1.4",
    "@types/jest": "^24.0.11",
    "@types/nedb": "^1.8.7",
    "@types/node": "^11.12.2",
    "@types/react": "^16.8.10",
    "@types/react-dom": "^16.8.3",
    "bootstrap": "^4.3.1",
    "electron-is-dev": "^1.0.1",
    "electron-settings": "^3.2.0",
    "jquery": "^3.3.1",
    "nedb": "^1.8.0",
    "popper.js": "^1.14.7",
    "react": "^16.8.4",
    "react-bootstrap": "^1.0.0-beta.6",
    "react-dom": "^16.8.4",
    "react-scripts": "2.1.8",
    "uuid": "^3.3.2"
  },
  "devDependencies": {
    "concurrently": "^4.1.0",
    "cross-env": "^5.2.0",
    "electron": "^4.1.0",
    "electron-builder": "^20.39.0",
    "electron-devtools-installer": "^2.2.4",
    "typescript": "^3.3.4000",
    "wait-on": "^3.2.0"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build && npm run build-ts",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "build-ts": "tsc --noEmit false -m CommonJS --outDir build && rm -r build/src",
    "electron": "electron .",
    "electron-dev": "concurrently --kill-others \"cross-env BROWSER=none npm run start\" \"npm run build-ts && wait-on http://localhost:3000 && cross-env ELECTRON_USE_WEB=true electron .\"",
    "electron-pack": "electron-builder --dir",
    "preelectron-pack": "npm run build"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "main": "build/electron/electron.js",
  "homepage": "./",
  "build": {
    "appId": "fair-stream-app",
    "extraMetadata": {
      "main": "build/electron/electron.js"
    },
    "files": [
      "electron/**/*",
      "build/**/*",
      "node_modules/**/*"
    ],
    "directories": {
      "buildResources": "assets"
    }
  }
}
